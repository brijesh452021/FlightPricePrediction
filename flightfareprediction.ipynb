from sklearn.model_selection import train_test_split

train_x,test_x,train_y,test_y=train_test_split(X,y,test_size=0.3,random_state=0)

from sklearn.model_selection import RandomizedSearchCV
from sklearn.ensemble import RandomForestRegressor
rf=RandomForestRegressor()

import numpy as np
n_estimators=[int(x) for x in np.linspace(start=100,stop=1000,num=10)]
criterion=['mse','mae']
max_depth=[1,2,5,10,20,100]
min_samples_split=[1,2,5,10,20,100]
min_samples_leaf=[1,2,5,10,20,100]
max_features=['auto','sqrt']
max_samples=[1,2,5,10,20,100]

param_dist={'n_estimators':n_estimators,
            'criterion':criterion,
            'max_depth':max_depth,
            'min_samples_split':min_samples_split,
            'min_samples_leaf':min_samples_leaf,
            'max_features':max_features,
            'max_samples':max_samples
            }

rf_random=RandomizedSearchCV(estimator=rf,param_distributions=param_dist,cv=5,verbose=2)
rf_random.fit(train_x,train_y)

rf_random.best_estimator_

rf_random.best_params_

rf_random.best_score_

predt=rf_random.predict(test_x)

plt.figure(figsize=(15,15))
plt.scatter(test_y,predt)
plt.show()

plt.figure(figsize=(15,15))
sns.distplot(test_y-predt)

from sklearn import metrics
print('MAE:', metrics.mean_absolute_error(test_y, predt))
print('MSE:', metrics.mean_squared_error(test_y, predt))

import pickle

pickle.dump(rf_random,open('FlightFarePrediction.pkl','wb'))

